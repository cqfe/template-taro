{
  "name": "template-taro",
  "version": "1.0.0",
  "private": true,
  "description": "tarojs + vuejs + nutui模版",
  "templateInfo": {
    "name": "vue3-NutUI",
    "typescript": false,
    "css": "Sass",
    "framework": "Vue3"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix",
    "prepare": "husky"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@cqfe/generate-api": "^0.11.13",
    "@nutui/nutui-taro": "4.3.13",
    "@tarojs/components": "4.0.7",
    "@tarojs/helper": "4.0.7",
    "@tarojs/plugin-framework-vue3": "4.0.7",
    "@tarojs/plugin-html": "4.0.7",
    "@tarojs/plugin-http": "4.0.7",
    "@tarojs/plugin-platform-alipay": "4.0.7",
    "@tarojs/plugin-platform-h5": "4.0.7",
    "@tarojs/plugin-platform-jd": "4.0.7",
    "@tarojs/plugin-platform-qq": "4.0.7",
    "@tarojs/plugin-platform-swan": "4.0.7",
    "@tarojs/plugin-platform-tt": "4.0.7",
    "@tarojs/plugin-platform-weapp": "4.0.7",
    "@tarojs/runtime": "4.0.7",
    "@tarojs/shared": "4.0.7",
    "@tarojs/taro": "4.0.7",
    "@vueuse/core": "11.2.0",
    "axios": "1.7.7",
    "dayjs": "^1.11.13",
    "vue": "3.5.12"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@commitlint/config-conventional": "^19.2.2",
    "@cqfe/eslint-config": "^1.3.3",
    "@cqfe/prettier-config": "^1.3.4",
    "@cqfe/stylelint-config": "^1.3.1",
    "@nutui/auto-import-resolver": "^1.0.0",
    "@tarojs/cli": "4.0.7",
    "@tarojs/webpack5-runner": "^4.0.7",
    "babel-preset-taro": "4.0.7",
    "commitlint": "^19.4.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.8",
    "postcss": "^8.4.18",
    "sass": "1.60.0",
    "terser": "^5.16.8",
    "unplugin-vue-components": "^0.26.0",
    "webpack": "^5.76.0"
  },
  "prettier": "@cqfe/prettier-config",
  "stylelint": {
    "extends": "@cqfe/stylelint-config"
  },
  "eslintConfig": {
    "extends": "@cqfe/eslint-config/lib/vue"
  },
  "lint-staged": {
    "*.{js,ts,vue,cjs,mjs}": [
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier -c --write"
    ]
  }
}
